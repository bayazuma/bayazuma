void tkbys_Add_float(float A, float B, out float Out)
{
    Out = A + B;
}
#pragma glslify: export(tkbys_Add_float)

void tkbys_Add_vec3(vec3 A, vec3 B, out vec3 Out)
{
    Out = A + B;
}
#pragma glslify: export(tkbys_Add_vec3)

void tkbys_Add_vec4(vec4 A, vec4 B, out vec4 Out)
{
    Out = A + B;
}
#pragma glslify: export(tkbys_Add_vec4)
